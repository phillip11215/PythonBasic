def add(a, b):
    return a + b
def sub(a, b):
    return a - b
def mul(a, b):
    return a * b
def div(a, b):
    return a // b
a = 6
b = 3
print(add(a,b)) #9
print(sub(a,b)) #3
print(mul(a,b)) #18
print(div(a,b)) #2
def plus(a, b):
    return a + b
def minus(a, b):
    return a - b
def multi(a, b):
    return a * b
def divi(a, b):
    return a // b
a = 6
b = 3
print(plus(b, a))
print(minus(b, a))
print(multi(b, a))
print(divi(b, a))
